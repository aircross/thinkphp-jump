{
    "name": "aircross/thinkphp-jump",
    "description": "适用于thinkphp8.0的跳转扩展,调试中,请慎用",
    "keywords": ["thinkphp", "think-jump", "success","error","result","redirect"],
    "license": "Apache-2.0",
    "type": "think-extend",
    "authors": [
        {
            "name": "aircross",
            "email": "aircross@do-not-email-me.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "topthink/framework": "^8.0",
        "topthink/think-view": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "aircross\\think\\": "src/"
        }
    },
    "extra": {
        "think": {
            "config": {
                "jump": "src/config/jump.php"
            }
        }
    }
}
